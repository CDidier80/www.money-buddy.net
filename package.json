{
  "name": "money-buddy",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Collin Didier <collin.didier80@gmail.com>",
  "nodemonConfig": {
    "ignore": [
      "client/"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:seed": "node ./db/seed.js",
    "build": "npx sequelize-cli db:migrate && cd client && rm -rf build && npm install && npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.2",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "prettier": "2.3.1",
    "sequelize-cli": "^6.2.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
